<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>AjaxServlet</servlet-name>
		<servlet-class>com.rl.clinicalcloud.servlets.AjaxServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AjaxServlet</servlet-name>
		<url-pattern>/AjaxServlet</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>PatientListServlet</servlet-name>
		<servlet-class>com.rl.clinicalcloud.servlets.PatientListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PatientListServlet</servlet-name>
		<url-pattern>/PatientListServlet</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>TrialVisitServlet</servlet-name>
		<servlet-class>com.rl.clinicalcloud.servlets.TrialVisitServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TrialVisitServlet</servlet-name>
		<url-pattern>/TrialVisitServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>VisitInfoServlet</servlet-name>
		<servlet-class>com.rl.clinicalcloud.servlets.VisitInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VisitInfoServlet</servlet-name>
		<url-pattern>/VisitInfoServlet</url-pattern>
	</servlet-mapping>	
</web-app>
